{
  "name": "angular-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for angular applications.",
  "scripts": {
    "start": "gulp --require coffee-script/register",
    "build": "gulp --require coffee-script/register build",
    "serve": "gulp --require coffee-script/register serve",
    "clean": "rimraf public",
    "reinstall": "npm cache clean && rimraf node_modules && pnpm-install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/angular-starter-kit.git"
  },
  "keywords": [
    "Angular",
    "Material",
    "Firebase"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/angular-starter-kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/angular-starter-kit#readme",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-material": "^1.0.5",
    "angularfire": "^1.1.3",
    "firebase": "^2.4.1"
  },
  "devDependencies": {
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-changed": "^1.3.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jade": "^1.1.0",
    "gulp-jadelint": "^0.1.0",
    "gulp-minify-html": "^1.0.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-stylint": "^3.0.0",
    "gulp-stylus": "^2.3.0",
    "gulp-uglify": "^1.5.2",
    "gulp-uglifycss": "^1.0.5",
    "imagemin-pngquant": "^4.2.0",
    "poststylus": "^0.2.3",
    "rucksack-css": "^0.8.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
