{
  "name": "angular-starter-kit",
  "version": "1.0.0",
  "description": "A starter kit for Angular applications",
  "scripts": {
    "start": "gulp --require coffee-script/register",
    "test": "karma start karma.conf.coffee",
    "build-dev": "gulp --require coffee-script/register build-dev",
    "build-test": "karma start karma.conf.coffee --single-run --browsers=PhantomJS",
    "build-dev-watch": "gulp --require coffee-script/register build-dev-watch",
    "build-test-watch": "karma start karma.conf.coffee --browsers=PhantomJS",
    "build-prod": "gulp --require coffee-script/register build-prod",
    "build-e2e": "protractor protractor.conf.coffee",
    "clean": "gulp --require coffee-script/register clean",
    "reinstall": "npm cache clean && git clean -dfx -e '.*' && npm install",
    "postinstall": "webdriver-manager update",
    "lint": "gulp --require coffee-script/register lint",
    "unit": "karma start karma.conf.coffee --single-run --browsers=Firefox",
    "e2e": "protractor protractor.conf.coffee --browser=firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/angular-starter-kit.git"
  },
  "keywords": [
    "Angular",
    "Material",
    "Firebase"
  ],
  "author": "Shyam Chen, 陳彥澄",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Shyam-Chen/angular-starter-kit/issues"
  },
  "homepage": "https://github.com/Shyam-Chen/angular-starter-kit#readme",
  "dependencies": {
    "angular": "^1.5.3",
    "angular-animate": "^1.5.3",
    "angular-aria": "^1.5.3",
    "angular-material": "^1.0.6",
    "angular-messages": "^1.5.3",
    "angularfire": "^1.1.4",
    "firebase": "^2.4.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.3",
    "browser-sync": "^2.11.2",
    "browserify": "^13.0.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-changed": "^1.3.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-jade": "^1.1.0",
    "gulp-jadelint": "^0.1.0",
    "gulp-minify-html": "^1.0.6",
    "gulp-notify": "^2.2.0",
    "gulp-rimraf": "^0.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-stylint": "^3.0.0",
    "gulp-stylus": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "gulp-uglifycss": "^1.0.6",
    "gulp-util": "^3.0.7",
    "imagemin-pngquant": "^4.2.2",
    "jasmine": "^2.4.1",
    "karma": "^0.13.22",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^0.2.3",
    "karma-coffee-preprocessor": "^0.3.0",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "merge-stream": "^1.0.0",
    "phantomjs": "^2.1.3",
    "phantomjs-prebuilt": "^2.1.7",
    "poststylus": "^0.2.3",
    "pretty-hrtime": "^1.0.2",
    "protractor": "^3.2.2",
    "rucksack-css": "^0.8.5",
    "run-sequence": "^1.1.5",
    "shelljs": "^0.6.0",
    "stream-combiner": "^0.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
}
